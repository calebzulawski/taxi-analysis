import httplib2
import csv
import simplekml

import polyline

with open('boundaries.csv', mode='r') as csvfile:
	reader = csv.reader(csvfile)
	neighborhood_dict = {rows[0]:rows[1:] for rows in reader}

kml = simplekml.Kml()

for key, value in neighborhood_dict.iteritems():
	boundary = polyline.decode(value[1])
	pol = kml.newpolygon(name=value[2], outerboundaryis=boundary)
kml.save('neighborhoods.kml')